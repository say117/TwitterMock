<div id='wrap'>
  <div id='header'>
    <% if flash[:notice] %>
      <p class='notice'><%= flash[:notice] %></p>
    <% end %>
  </div><!-- /header -->
  <div id='innner'>

    <div id='sideWrap'>
      <% if session[:user_id].nil? %>
        <%= render 'sessions/form' %>
        <%= link_to 'Sign up', new_user_path %>
      <% else %>
        <h4>UserName</h4>
        <%= @user.name %><br>
        <%= link_to 'Sign out', sessions_destroy_path %>
        <%= render 'bweets/form' %>
      <% end %>
    </div><!-- /sideWrap -->
    <div id='mainWrap'>
      <h1>Bweets</h1>
      <div id='bweet'>
        <%= render partial: "bweets", collections: @bweets %>
      </div>
      <%= button_to 'More Show 10 Bweets' %>

    </div><!-- /mainWrap -->
  </div><!-- inner -->

  <div id='footer'>
    
  </div><!-- /footer -->
</div><!-- /wrap -->