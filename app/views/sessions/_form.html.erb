<%= form_tag('/sessions/create') do %>
<fieldset>
  <legend>Sign in</legend>
  
  <div>
  <%= label_tag :name %>
  <%= text_field_tag :name%>
  </div>
  <div>
  <%= label_tag :password %>
  <%= password_field_tag :password%>
  </div>
  <div>
    <%= submit_tag 'Sign in' %>
  </div>
</fieldset>
<% end %>